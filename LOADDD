local script_key = _G.script_key or script_key
if not script_key then
    warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£ script_key")
    return
end

local hwid = game:GetService("RbxAnalyticsService"):GetClientId()
print("üîç Scanning HWID:", hwid)

-- Services
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local SoundService = game:GetService("SoundService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Colors (Hacker Theme)
local COLORS = {
    BACKGROUND = Color3.fromRGB(5, 5, 5),
    PANEL = Color3.fromRGB(10, 15, 10),
    ACCENT = Color3.fromRGB(0, 255, 65),
    TEXT = Color3.fromRGB(0, 255, 65),
    ERROR = Color3.fromRGB(255, 50, 50),
    WARNING = Color3.fromRGB(255, 255, 0),
    SHADOW = Color3.fromRGB(0, 0, 0)
}

-- Fonts
local FONTS = {
    MAIN = Enum.Font.Code,
    BOLD = Enum.Font.CodeBold,
    TITLE = Enum.Font.Arcade
}

-- Create Matrix Rain Effect
local function createMatrixRain()
    local matrixGui = Instance.new("ScreenGui")
    matrixGui.Name = "MatrixRain"
    matrixGui.Parent = playerGui
    matrixGui.ResetOnSpawn = false
    
    for i = 1, 15 do
        local column = Instance.new("Frame")
        column.Name = "Column" .. i
        column.Size = UDim2.new(0, 20, 1, 0)
        column.Position = UDim2.new(0, (i-1) * 80, 0, 0)
        column.BackgroundTransparency = 1
        column.Parent = matrixGui
        
        spawn(function()
            while matrixGui.Parent do
                local char = Instance.new("TextLabel")
                char.Size = UDim2.new(0, 20, 0, 20)
                char.Position = UDim2.new(0, 0, 0, -20)
                char.BackgroundTransparency = 1
                char.Text = string.char(math.random(33, 126))
                char.TextColor3 = COLORS.ACCENT
                char.TextTransparency = 0.3
                char.Font = FONTS.MAIN
                char.TextSize = 14
                char.Parent = column
                
                local tween = TweenService:Create(char, 
                    TweenInfo.new(math.random(2, 5), Enum.EasingStyle.Linear),
                    {Position = UDim2.new(0, 0, 1, 20), TextTransparency = 1}
                )
                tween:Play()
                
                tween.Completed:Connect(function()
                    char:Destroy()
                end)
                
                wait(math.random(1, 3) / 10)
            end
        end)
    end
    
    return matrixGui
end

-- Create HWID Display GUI
local function createHWIDGUI(hwid)
    -- Remove existing GUI
    local existingGui = playerGui:FindFirstChild("HWIDVerifyGUI")
    if existingGui then
        existingGui:Destroy()
    end
    
    -- Create Matrix Background
    local matrixGui = createMatrixRain()
    
    -- Main GUI
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "HWIDVerifyGUI"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = playerGui
    
    -- Blur Effect
    local blur = Instance.new("BlurEffect")
    blur.Size = 10
    blur.Parent = game:GetService("Lighting")
    
    -- Background Overlay
    local overlay = Instance.new("Frame")
    overlay.Name = "Overlay"
    overlay.Size = UDim2.new(1, 0, 1, 0)
    overlay.Position = UDim2.new(0, 0, 0, 0)
    overlay.BackgroundColor3 = COLORS.BACKGROUND
    overlay.BackgroundTransparency = 0.3
    overlay.BorderSizePixel = 0
    overlay.Parent = screenGui
    
    -- Main Container
    local mainFrame = Instance.new("Frame")
    mainFrame.Name = "MainFrame"
    mainFrame.Size = UDim2.new(0, 600, 0, 450)
    mainFrame.Position = UDim2.new(0.5, -300, 0.5, -225)
    mainFrame.BackgroundColor3 = COLORS.PANEL
    mainFrame.BorderSizePixel = 0
    mainFrame.Parent = screenGui
    
    -- Corner Radius
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = mainFrame
    
    -- Shadow Effect
    local shadow = Instance.new("ImageLabel")
    shadow.Name = "Shadow"
    shadow.Size = UDim2.new(1, 20, 1, 20)
    shadow.Position = UDim2.new(0, -10, 0, -10)
    shadow.BackgroundTransparency = 1
    shadow.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
    shadow.ImageColor3 = COLORS.SHADOW
    shadow.ImageTransparency = 0.8
    shadow.ZIndex = mainFrame.ZIndex - 1
    shadow.Parent = mainFrame
    
    -- Border Glow
    local borderGlow = Instance.new("UIStroke")
    borderGlow.Color = COLORS.ACCENT
    borderGlow.Thickness = 2
    borderGlow.Transparency = 0.5
    borderGlow.Parent = mainFrame
    
    -- Header
    local header = Instance.new("Frame")
    header.Name = "Header"
    header.Size = UDim2.new(1, 0, 0, 80)
    header.Position = UDim2.new(0, 0, 0, 0)
    header.BackgroundColor3 = Color3.fromRGB(0, 10, 0)
    header.BorderSizePixel = 0
    header.Parent = mainFrame
    
    local headerCorner = Instance.new("UICorner")
    headerCorner.CornerRadius = UDim.new(0, 8)
    headerCorner.Parent = header
    
    -- Fix header corners
    local headerFix = Instance.new("Frame")
    headerFix.Size = UDim2.new(1, 0, 0, 40)
    headerFix.Position = UDim2.new(0, 0, 1, -40)
    headerFix.BackgroundColor3 = Color3.fromRGB(0, 10, 0)
    headerFix.BorderSizePixel = 0
    headerFix.Parent = header
    
    -- Title
    local title = Instance.new("TextLabel")
    title.Name = "Title"
    title.Size = UDim2.new(1, -40, 0, 40)
    title.Position = UDim2.new(0, 20, 0, 10)
    title.BackgroundTransparency = 1
    title.Text = "‚ö†Ô∏è ACCESS DENIED"
    title.TextColor3 = COLORS.ERROR
    title.TextSize = 28
    title.Font = FONTS.TITLE
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.Parent = header
    
    -- Subtitle
    local subtitle = Instance.new("TextLabel")
    subtitle.Name = "Subtitle"
    subtitle.Size = UDim2.new(1, -40, 0, 25)
    subtitle.Position = UDim2.new(0, 20, 0, 45)
    subtitle.BackgroundTransparency = 1
    subtitle.Text = "HWID VERIFICATION FAILED"
    subtitle.TextColor3 = COLORS.TEXT
    subtitle.TextSize = 16
    subtitle.Font = FONTS.MAIN
    subtitle.TextXAlignment = Enum.TextXAlignment.Left
    subtitle.Parent = header
    
    -- Status LED
    local statusLED = Instance.new("Frame")
    statusLED.Name = "StatusLED"
    statusLED.Size = UDim2.new(0, 12, 0, 12)
    statusLED.Position = UDim2.new(1, -30, 0, 20)
    statusLED.BackgroundColor3 = COLORS.ERROR
    statusLED.BorderSizePixel = 0
    statusLED.Parent = header
    
    local ledCorner = Instance.new("UICorner")
    ledCorner.CornerRadius = UDim.new(0.5, 0)
    ledCorner.Parent = statusLED
    
    -- Blinking LED effect
    spawn(function()
        while statusLED.Parent do
            TweenService:Create(statusLED, TweenInfo.new(0.8), {BackgroundTransparency = 0.7}):Play()
            wait(0.8)
            TweenService:Create(statusLED, TweenInfo.new(0.8), {BackgroundTransparency = 0}):Play()
            wait(0.8)
        end
    end)
    
    -- Message Section
    local messageFrame = Instance.new("Frame")
    messageFrame.Name = "MessageFrame"
    messageFrame.Size = UDim2.new(1, -40, 0, 100)
    messageFrame.Position = UDim2.new(0, 20, 0, 100)
    messageFrame.BackgroundTransparency = 1
    messageFrame.Parent = mainFrame
    
    local messageText = Instance.new("TextLabel")
    messageText.Name = "MessageText"
    messageText.Size = UDim2.new(1, 0, 1, 0)
    messageText.Position = UDim2.new(0, 0, 0, 0)
    messageText.BackgroundTransparency = 1
    messageText.Text = "[ SYSTEM ] Your Hardware ID is not registered in our database.\n[ INFO ] Please copy your HWID and contact our support team.\n[ STATUS ] Authentication Required"
    messageText.TextColor3 = COLORS.TEXT
    messageText.TextSize = 14
    messageText.Font = FONTS.MAIN
    messageText.TextWrapped = true
    messageText.TextXAlignment = Enum.TextXAlignment.Left
    messageText.TextYAlignment = Enum.TextYAlignment.Top
    messageText.Parent = messageFrame
    
    -- HWID Display Section
    local hwidSection = Instance.new("Frame")
    hwidSection.Name = "HWIDSection"
    hwidSection.Size = UDim2.new(1, -40, 0, 120)
    hwidSection.Position = UDim2.new(0, 20, 0, 220)
    hwidSection.BackgroundColor3 = Color3.fromRGB(2, 2, 2)
    hwidSection.BorderSizePixel = 0
    hwidSection.Parent = mainFrame
    
    local hwidCorner = Instance.new("UICorner")
    hwidCorner.CornerRadius = UDim.new(0, 4)
    hwidCorner.Parent = hwidSection
    
    local hwidBorder = Instance.new("UIStroke")
    hwidBorder.Color = COLORS.ACCENT
    hwidBorder.Thickness = 1
    hwidBorder.Transparency = 0.7
    hwidBorder.Parent = hwidSection
    
    -- HWID Label
    local hwidLabel = Instance.new("TextLabel")
    hwidLabel.Name = "HWIDLabel"
    hwidLabel.Size = UDim2.new(1, -20, 0, 30)
    hwidLabel.Position = UDim2.new(0, 10, 0, 10)
    hwidLabel.BackgroundTransparency = 1
    hwidLabel.Text = "[ HARDWARE_ID ]"
    hwidLabel.TextColor3 = COLORS.ACCENT
    hwidLabel.TextSize = 12
    hwidLabel.Font = FONTS.BOLD
    hwidLabel.TextXAlignment = Enum.TextXAlignment.Left
    hwidLabel.Parent = hwidSection
    
    -- HWID Value
    local hwidValue = Instance.new("TextLabel")
    hwidValue.Name = "HWIDValue"
    hwidValue.Size = UDim2.new(1, -20, 0, 50)
    hwidValue.Position = UDim2.new(0, 10, 0, 35)
    hwidValue.BackgroundTransparency = 1
    hwidValue.Text = hwid
    hwidValue.TextColor3 = COLORS.TEXT
    hwidValue.TextSize = 16
    hwidValue.Font = FONTS.MAIN
    hwidValue.TextWrapped = true
    hwidValue.TextXAlignment = Enum.TextXAlignment.Left
    hwidValue.Parent = hwidSection
    
    -- Copy Button
    local copyButton = Instance.new("TextButton")
    copyButton.Name = "CopyButton"
    copyButton.Size = UDim2.new(0, 200, 0, 40)
    copyButton.Position = UDim2.new(0, 20, 0, 365)
    copyButton.BackgroundColor3 = Color3.fromRGB(0, 40, 0)
    copyButton.BorderSizePixel = 0
    copyButton.Text = "[ COPY_HWID ]"
    copyButton.TextColor3 = COLORS.ACCENT
    copyButton.TextSize = 14
    copyButton.Font = FONTS.BOLD
    copyButton.Parent = mainFrame
    
    local copyCorner = Instance.new("UICorner")
    copyCorner.CornerRadius = UDim.new(0, 4)
    copyCorner.Parent = copyButton
    
    local copyStroke = Instance.new("UIStroke")
    copyStroke.Color = COLORS.ACCENT
    copyStroke.Thickness = 1
    copyStroke.Transparency = 0.5
    copyStroke.Parent = copyButton
    
    -- Discord Button
    local discordButton = Instance.new("TextButton")
    discordButton.Name = "DiscordButton"
    discordButton.Size = UDim2.new(0, 200, 0, 40)
    discordButton.Position = UDim2.new(0, 240, 0, 365)
    discordButton.BackgroundColor3 = Color3.fromRGB(0, 0, 40)
    discordButton.BorderSizePixel = 0
    discordButton.Text = "[ DISCORD_SUPPORT ]"
    discordButton.TextColor3 = Color3.fromRGB(100, 150, 255)
    discordButton.TextSize = 14
    discordButton.Font = FONTS.BOLD
    discordButton.Parent = mainFrame
    
    local discordCorner = Instance.new("UICorner")
    discordCorner.CornerRadius = UDim.new(0, 4)
    discordCorner.Parent = discordButton
    
    local discordStroke = Instance.new("UIStroke")
    discordStroke.Color = Color3.fromRGB(100, 150, 255)
    discordStroke.Thickness = 1
    discordStroke.Transparency = 0.5
    discordStroke.Parent = discordButton
    
    -- Close Button
    local closeButton = Instance.new("TextButton")
    closeButton.Name = "CloseButton"
    closeButton.Size = UDim2.new(0, 120, 0, 40)
    closeButton.Position = UDim2.new(0, 460, 0, 365)
    closeButton.BackgroundColor3 = Color3.fromRGB(40, 0, 0)
    closeButton.BorderSizePixel = 0
    closeButton.Text = "[ EXIT ]"
    closeButton.TextColor3 = COLORS.ERROR
    closeButton.TextSize = 14
    closeButton.Font = FONTS.BOLD
    closeButton.Parent = mainFrame
    
    local closeCorner = Instance.new("UICorner")
    closeCorner.CornerRadius = UDim.new(0, 4)
    closeCorner.Parent = closeButton
    
    local closeStroke = Instance.new("UIStroke")
    closeStroke.Color = COLORS.ERROR
    closeStroke.Thickness = 1
    closeStroke.Transparency = 0.5
    closeStroke.Parent = closeButton
    
    -- Typewriter Effect for Message
    local originalText = messageText.Text
    messageText.Text = ""
    spawn(function()
        for i = 1, #originalText do
            messageText.Text = string.sub(originalText, 1, i)
            wait(0.03)
        end
    end)
    
    -- Button Animations
    local function animateButton(button, hoverColor, normalColor)
        button.MouseEnter:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = hoverColor}):Play()
            TweenService:Create(button, TweenInfo.new(0.1), {Size = button.Size + UDim2.new(0, 4, 0, 2)}):Play()
        end)
        
        button.MouseLeave:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = normalColor}):Play()
            TweenService:Create(button, TweenInfo.new(0.1), {Size = button.Size - UDim2.new(0, 4, 0, 2)}):Play()
        end)
    end
    
    animateButton(copyButton, Color3.fromRGB(0, 60, 0), Color3.fromRGB(0, 40, 0))
    animateButton(discordButton, Color3.fromRGB(0, 20, 60), Color3.fromRGB(0, 0, 40))
    animateButton(closeButton, Color3.fromRGB(60, 0, 0), Color3.fromRGB(40, 0, 0))
    
    -- Button Events
    copyButton.MouseButton1Click:Connect(function()
        setclipboard(hwid)
        copyButton.Text = "[ COPIED! ]"
        copyButton.TextColor3 = COLORS.WARNING
        
        -- Success sound effect
        spawn(function()
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxasset://sounds/electronicpingshort.wav"
            sound.Volume = 0.5
            sound.Parent = SoundService
            sound:Play()
            sound.Ended:Wait()
            sound:Destroy()
        end)
        
        wait(2)
        copyButton.Text = "[ COPY_HWID ]"
        copyButton.TextColor3 = COLORS.ACCENT
    end)
    
    discordButton.MouseButton1Click:Connect(function()
        setclipboard("https://discord.gg/YOUR_DISCORD_INVITE") -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô Discord ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì
        discordButton.Text = "[ LINK_COPIED ]"
        discordButton.TextColor3 = COLORS.WARNING
        wait(2)
        discordButton.Text = "[ DISCORD_SUPPORT ]"
        discordButton.TextColor3 = Color3.fromRGB(100, 150, 255)
    end)
    
    closeButton.MouseButton1Click:Connect(function()
        -- Fade out animation
        local fadeOut = TweenService:Create(screenGui, TweenInfo.new(0.5), {BackgroundTransparency = 1})
        local frameOut = TweenService:Create(mainFrame, TweenInfo.new(0.5), {
            BackgroundTransparency = 1,
            Size = UDim2.new(0, 0, 0, 0),
            Position = UDim2.new(0.5, 0, 0.5, 0)
        })
        
        fadeOut:Play()
        frameOut:Play()
        
        frameOut.Completed:Connect(function()
            screenGui:Destroy()
            matrixGui:Destroy()
            blur:Destroy()
        end)
    end)
    
    -- Entrance Animation
    mainFrame.Size = UDim2.new(0, 0, 0, 0)
    mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    mainFrame.BackgroundTransparency = 1
    
    local sizeIn = TweenService:Create(mainFrame, TweenInfo.new(0.8, Enum.EasingStyle.Back), {
        Size = UDim2.new(0, 600, 0, 450),
        Position = UDim2.new(0.5, -300, 0.5, -225)
    })
    
    local fadeIn = TweenService:Create(mainFrame, TweenInfo.new(0.5), {BackgroundTransparency = 0})
    
    sizeIn:Play()
    fadeIn:Play()
    
    return screenGui
end

-- Main Logic
local success, data = pcall(function()
    return game:HttpGet("https://pastebin.com/raw/hRw8UU9e") -- ‡∏•‡∏¥‡∏á‡∏Å‡πå Pastebin ‡∏ó‡∏µ‡πà‡πÄ‡∏Å‡πá‡∏ö key:hwid
end)

if not success then
    warn("‚ùå ‡∏î‡∏∂‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏Ñ‡∏µ‡∏¢‡πå‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ: " .. tostring(data))
    return
end

print("üîê Verifying credentials...")

local validKeys = {}
for line in data:gmatch("[^\r\n]+") do
    local k, v = line:match("([^:]+):(.+)")
    if k and v then
        validKeys[k] = v
    end
end

if validKeys[script_key] == hwid then
    print("‚úÖ Key ‡πÅ‡∏•‡∏∞ HWID ‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏à‡∏£‡∏¥‡∏á...")
    
    -- Success notification
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "üîì ACCESS GRANTED";
        Text = "Authentication successful. Loading main script...";
        Duration = 3;
    })
    
    -- ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏à‡∏£‡∏¥‡∏á
    wait(1)
    print("DONE NiGGA")
else
    warn("‚ùå Key ‡∏´‡∏£‡∏∑‡∏≠ HWID ‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á")
    
    -- Error notification
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "üîí ACCESS DENIED";
        Text = "Invalid credentials. Please verify your HWID.";
        Duration = 5;
    })
    
    -- ‡πÅ‡∏™‡∏î‡∏á HWID GUI
    createHWIDGUI(hwid)
end
