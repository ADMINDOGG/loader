local script_key = _G.script_key or script_key
if not script_key then
    warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£ script_key")
    return
end

local hwid = game:GetService("RbxAnalyticsService"):GetClientId()
local player = game:GetService("Players").LocalPlayer
local HttpService = game:GetService("HttpService")

-- ‡∏î‡∏∂‡∏á‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£ key/hwid ‡∏à‡∏≤‡∏Å Pastebin
local success, data = pcall(function()
    return game:HttpGet("https://pastebin.com/raw/hRw8UU9e")
end)

if not success then
    warn("‚ùå ‡∏î‡∏∂‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏Ñ‡∏µ‡∏¢‡πå‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ: " .. tostring(data))
    return
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ï‡∏≤‡∏£‡∏≤‡∏á‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö
local validKeys = {}
for line in data:gmatch("[^\r\n]+") do
    local k, v = line:match("([^:]+):(.+)")
    if k and v then
        validKeys[k] = v
    end
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö key/hwid
if validKeys[script_key] == hwid then
    print("‚úÖ Key ‡πÅ‡∏•‡∏∞ HWID ‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏à‡∏£‡∏¥‡∏á...")

    -- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÑ‡∏õ‡∏¢‡∏±‡∏á Discord Webhook
    local webhookUrl = "https://discord.com/api/webhooks/1375147331980099594/5758wuuuL-84m7Vw1u1Ztvi9iqlR-40CbS0UtbCTt56fknqZauFZ62AVZ27EX8xvGd2c"
    local payload = {
        content = "**‚úÖ ‡πÄ‡∏Ç‡πâ‡∏≤‡∏ñ‡∏∂‡∏á‡∏£‡∏∞‡∏ö‡∏ö‡∏î‡πâ‡∏ß‡∏¢ Key ‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á**",
        embeds = {{
            title = "Key Access Log",
            color = 65280,
            fields = {
                { name = "üë§ Username", value = player.Name, inline = true },
                { name = "üîë Key", value = script_key, inline = true },
                { name = "üíª HWID", value = hwid, inline = false }
            }
        }}
    }

    pcall(function()
        request({
            Url = webhookUrl,
            Method = "POST",
            Headers = { ["Content-Type"] = "application/json" },
            Body = HttpService:JSONEncode(payload)
        })
    end)

    -- ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏´‡∏•‡∏±‡∏Å
    print("DONE")
else
    warn("‚ùå Key ‡∏´‡∏£‡∏∑‡∏≠ HWID ‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á")
end
